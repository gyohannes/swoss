<p id="notice"><%= notice %></p>

<h1>Admission Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Admission</th>
      <th>Status date</th>
      <th>Status date gr</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admission_statuses.each do |admission_status| %>
      <tr>
        <td><%= admission_status.admission %></td>
        <td><%= admission_status.status_date %></td>
        <td><%= admission_status.status_date_gr %></td>
        <td><%= admission_status.status %></td>
        <td><%= link_to 'Show', admission_status %></td>
        <td><%= link_to 'Edit', edit_admission_status_path(admission_status) %></td>
        <td><%= link_to 'Destroy', admission_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admission Status', new_admission_status_path %>
